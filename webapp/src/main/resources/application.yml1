server:
  port: 8080
spring:
  application:
    name: webapp
eureka:
  client:
    serviceUrl:
      defaultZone: http://localhost:9091/eureka/
      
ssoServiceUrl: http://localhost:8085
security:
  # Disable Spring Boot basic authentication
  basic:
    enabled: false
  oauth2:
    sso:
      loginPath: /auth/login
    client:
      clientId: clientId
      clientSecret: secret
      accessTokenUrl: ${ssoServiceUrl}/oauth/token
      userAuthorizatioUrl: ${ssoServiceUrl}/oauth/authorize
    resource:
      jwt:
        keyUri: ${ssoServiceUrl}/oauth/token_key
        #key-value: 
  sessions: ALWAYS
zuul:
  routes:
    authorizationserver:
      path: /auth/**
      stripPrefix: false
    resource-center:
      path: /resource/**
      
